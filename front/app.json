{
  "expo": {
    "name": "Pharmacy Management",
    "slug": "pharmacy-management",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],

    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pharmacy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    },
    "packagerOpts": {
      "hostType": "tunnel",
      "devServer": {
        "host": "192.168.56.1"
      }
    },
    "android": {
      "package": "com.testtttttest.pharmacymanagement"
    }
  }
}
